{
  "name": "client",
  "version": "0.1.0",
  "description": "News App",
  "private": true,
  "dependencies": {
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "sass-loader": "^8.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "startr": "npm-run-all --parallel start watch:sass",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass src/sass/main.scss src/App.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/App.comp.css",
    "clean": "rm package-lock.json && rm -Rf node_modules"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.2",
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
